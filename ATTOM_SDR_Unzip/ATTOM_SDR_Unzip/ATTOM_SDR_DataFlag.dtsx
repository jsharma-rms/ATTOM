                Name="ScriptMain.cs"
                Encoding="UTF8"><![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using Microsoft.SqlServer.Management.Smo;
using System.Collections.Specialized;

namespace ST_bc5f848bcdc541058a8a4ab93994ffb2.csproj
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {

        #region VSTA generated code
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

        /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

        public void Main()
        {
            // TODO: Add your code here
            string strScript = null;
            StringCollection sc = new StringCollection();

            /* With ScriptingOptions you can specify different scripting  
            * options, for example to include IF NOT EXISTS, DROP  
            * statements, output location etc*/
            scriptOptions = new ScriptingOptions();
            scriptOptions.IncludeIfNotExists = true;
            ScriptingOptions scriptOptionsForDependendencies = new ScriptingOptions();
            scriptOptionsForDependendencies.Indexes = true;
            scriptOptionsForDependendencies.DriAll = true;
            scriptOptions.IncludeIfNotExists = true;

            string strTableName = Dts.Variables["varTargetTableName"].Value.ToString();
            //string strConMgr = Dts.Variables["conES_SDR_DB"].Value.ToString();
            string strVersion = Dts.Variables["varATTOMSourceVersion"].Value.ToString();

            string str_con;
            str_con = Dts.Connections["ES_SDR_DB"].ConnectionString.ToString();
            System.Data.OleDb.OleDbConnection con = new System.Data.OleDb.OleDbConnection(str_con);

            

            con.Open();

            Dts.Variables["varSourceDatabaseName"].Value = con.Database;
            Dts.Variables["varTargetDatabaseName"].Value = con.Database;

            Server myServer = new Server(con.DataSource);
            Scripter scripter = new Scripter(myServer);
            Database myDatabase = myServer.Databases[con.Database];

            foreach (Table myTable in myDatabase.Tables)
            {
                /* Generating IF EXISTS and DROP command for tables */
                if (myTable.Name.ToUpper().ToString() == strTableName.ToUpper().ToString())
                {
                    StringCollection tableScripts = myTable.Script(scriptOptions);
                    /* Generating CREATE TABLE command */
                    tableScripts = myTable.Script(scriptOptionsForDependendencies);
                    foreach (string script in tableScripts)
                        //Console.WriteLine(script);
                        strScript = strScript + "  " + script;
                }
            }

            strScript = strScript.ToUpper().ToString().Replace(strTableName.ToUpper().ToString(), strTableName.ToUpper().ToString() + "_Purge");                  
            System.Data.OleDb.OleDbCommand cmd = new System.Data.OleDb.OleDbCommand("IF OBJECT_ID('dbo." + strTableName + "_Purge" + "' ,'U') IS NOT NULL DROP TABLE dbo.[" + strTableName + "_Purge]" + "\n " + strScript, con);
            cmd.ExecuteNonQuery();
            Dts.TaskResult = (int)ScriptResults.Success;
        }

        public ScriptingOptions scriptOptions { get; set; }
    }
}]]></ProjectItem>
